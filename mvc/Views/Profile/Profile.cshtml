@model UserView
@{
    ViewData["Title"] = "Profile";
}
<table class="table table-striped table-bordered border-primary">
    <thead>
    <tr>
        
        <th>UserName</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Email</th>
        <th>Status</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <td user-for="@Model.UserName"></td>
        <td user-for="@Model.FirstName"></td>
        <td user-for="@Model.LastName"></td>
        <td user-for="@Model.Email"></td>
        <td user-for="@Model.Status"></td>     
    </tr>
    </tbody>
    <tr>
</table>

<a asp-controller="Home" asp-action="Index" button="Primary">Home</a>
