@model CreateOrganizationDto

<h1>Create post</h1>

<form method="post" enctype="multipart/form-data">
  <div class="mb-3">
    <label asp-for="@Model.Name" class="form-label">Name</label>
    <input type="text" asp-for="@Model.Name" class="form-control" placeholder="Name">
  </div>
  <div class="mb-3">
    <label asp-for="@Model.Address" class="form-label">Address</label>
    <input type="text" asp-for="@Model.Address" class="form-control" placeholder="Address">
  </div>
  <div class="mb-3">
    <label asp-for="@Model.PhoneNumber" class="form-label">PhoneNumber</label>
    <input type="text" asp-for="@Model.PhoneNumber" class="form-control" placeholder="PhoneNumber">
  </div>
  <div class="mb-3">
    <label asp-for="@Model.Image" class="form-label">Image</label>
    <input type="file" accept="image/*" asp-for="@Model.Image" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Create</button>
</form>